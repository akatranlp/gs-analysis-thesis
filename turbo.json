    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": [
                "dist/**",
                "build/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "dev": {
            "cache": false
        },
        "clean": {
            "cache": false
        }
    }
}