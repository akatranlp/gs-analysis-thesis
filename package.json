{
  "name": "gs-analysis-root",
  "version": "1.0.0",
  "private": true,
  "description": "Repo zu meiner Bachelorthesis an der Hochschule Flensburg",
  "scripts": {
    "build": "turbo run build",
    "prod-build": "turbo run prod-build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docker": "docker compose up -d",
    "docker-app": "docker build -t app . && docker run -it -p 3001:3000 --rm app"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akatranlp/gs-analysis.git"
  },
  "module": "commonjs",
  "keywords": [],
  "author": "akatranlp",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/akatranlp/gs-analysis/issues"
  },
  "homepage": "https://github.com/akatranlp/gs-analysis#readme"
}